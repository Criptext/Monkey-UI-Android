        <attr name="incoming" format="boolean" />
        <attr name="datetext" format="string" />
        <attr name="datetextColor" format="integer" />
        <attr name="datetextSize" format="string" />
        <attr name="bubble_bg" format="integer" />
        <attr name="datetimeText" format="string" />
        <attr name="datetimeColor" format="integer" />
        <attr name="datetimeSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="InputView">
        <attr name="attachmentDrawable" format="integer" />
        <attr name="sendTextDrawable" format="integer" />
        <attr name="sendAudioDrawable" format="integer" />
        <attr name="editTextDrawable" format="integer" />
        <attr name="backgroundDrawable" format="integer" />

